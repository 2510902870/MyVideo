<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.etc.myvideo.user.entity.User" table="user">
		<id name="uid">
			<generator class="native" />
		</id>
		<property name="uName" />
		<property name="uPassword" />
		<property name="uNickName" />
		<property name="uSex" />
		<property name="uMail" />
		<property name="uBirthday" />
		<property name="uQue" />
		<property name="uAns" />
		<property name="uAccumulate" not-null="true" />
		<property name="uLive" not-null="true" />
		<property name="state" />
		<property name="code" />
	</class>
</hibernate-mapping>