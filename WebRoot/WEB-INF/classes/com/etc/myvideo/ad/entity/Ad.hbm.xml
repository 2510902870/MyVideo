<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.etc.myvideo.ad.entity.Ad" table="ad" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="adimage" type="java.lang.String">
            <column name="adimage" />
        </property>
        <property name="adzhi" type="java.lang.String">
            <column name="adzhi" />
        </property>
    </class>
</hibernate-mapping>
